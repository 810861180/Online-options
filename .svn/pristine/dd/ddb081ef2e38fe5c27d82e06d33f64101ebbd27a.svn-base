{
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "git.ignoreMissingGitWarning": true,
    "editor.minimap.enabled": false,
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 20,
    "editor.tabSize": 2,
    //"editor.fontFamily": "Monaco,Menlo, 'Courier New', monospace",
    "explorer.confirmDelete": false, //TODO:dingyuhong
    "diffEditor.ignoreTrimWhitespace": false,
    "vsicons.dontShowNewVersionMessage": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "vetur.format.defaultFormatter.js": "vscode-typescript",
    "element-helper.language": "zh-CN",
    "element-helper.version": "2.4",
    "element-helper.indent-size": 2,
    "element-helper.quotes": "double", // html vue qoutes
    "element-helper.pug-quotes": "single", // jade/pug quotes
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 200,
    "files.insertFinalNewline": true,
    "eslint.autoFixOnSave": true,
    "emmet.includeLanguages": {
        "vue-html": "html"
    },
    "vetur.format.defaultFormatterOptions": {
        "js-beautify-html": {
            "wrap_attributes": "auto",
            "unformatted": [
                "area",
                "base",
                "br",
                "col",
                "embed",
                "hr",
                "input",
                "keygen",
                "link",
                "menuitem",
                "meta",
                "param",
                "source",
                "track",
                "wbr"
            ]
        }
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "window.zoomLevel": 0,
    "html.format.endWithNewline": true,
    "workbench.sideBar.location": "left",
    "breadcrumbs.enabled": true
}
